[global]
    # Enable streaming
    enable_streaming = yes

[stream]
    # Set the DNS name for the parent node
    enabled = yes
    bind to = netdata:19999
    default port = 19999
    allow from = *

[global]
    # Enable streaming
    enable_streaming = yes

[stream]
    # Set the DNS name for the parent node
    enabled = yes
    destination = netdata:19999
    hostname = redis